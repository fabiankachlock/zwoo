{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "devPath": "http://localhost:8080/",
    "distDir": "../dist-tmp"
  },
  "package": {
    "productName": "zwoo",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "globalShortcut": {
        "all": true
      },
      "fs": {
        "scope": ["$RESOURCE/*"]
      },
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "category": "CardGame",
      "copyright": "© IGD 2.0 UG (haftungsbeschränkt) 2024",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico",
        "icons/icon.png"
      ],
      "identifier": "de.igd20ug.zwoo",
      "longDescription": "zwoo",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [
        "../../backend/Zwoo.Backend.LocalServer/bin/Release/net8.0/osx-x64/native/Zwoo.Backend.LocalServer.exe"
      ],
      "shortDescription": "zwoo - The Second Challenge",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "zwoo",
        "width": 900
      }
    ]
  }
}
