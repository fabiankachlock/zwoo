{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "VUE_APP_USE_BACKEND=true vue-cli-service build",
    "build:preview": "VUE_APP_USE_BACKEND=false vue-cli-service build --dest dist-preview",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|ts|vue)\" --locales \"./src/locales/**/*.json\"",
    "format": "prettier --write \"src/**/*.{ts,vue,html,css,d.ts}\"",
    "prepare": "cd .. && husky install frontend/.husky"
  },
  "dependencies": {
    "autoprefixer": "^9",
    "core-js": "^3.20.1",
    "pinia": "^2.0.0-rc.9",
    "postcss": "^7",
    "register-service-worker": "^1.7.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "vue": "^3.2.26",
    "vue-i18n": "^9.1.0",
    "vue-router": "^4.0.0-0"
  },
  "lint-staged": {
    "src/**/!(*test).(ts|vue|html)": [
      "yarn lint"
    ],
    "src/**/*.(ts|vue|css|html)": [
      "yarn format"
    ]
  },
  "devDependencies": {
    "@iconify/vue": "^3.0.0",
    "@intlify/vue-i18n-loader": "^4.1.0",
    "@types/jest": "^24.0.19",
    "@types/vue-router": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-e2e-cypress": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.2.26",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.5.1",
    "typescript": "~4.1.5",
    "vue-cli-plugin-i18n": "~2.1.3",
    "vue-jest": "^5.0.0-0",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
